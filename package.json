{
  "name": "adventofcode",
  "version": "1.0.0",
  "description": "Advent of Code",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "2015": "node src/AoC15/AoC2015solve.js",
    "2021": "rm -rf ./Results.txt && node src/AoC21/AoC2021solve.js",
    "2022": "rm -rf ./Results.txt && node src/AoC22/AoC2022solve.js",
    "2023": "rm -rf ./dist && mkdir ./dist && tsc && cp -r src/AoC23/puzzleInputs dist/AoC23/ && node dist/AoC23/AoC2023solve.js",
    "2024": "rm -rf ./Results.txt && node src/AoC24/AoC2024solve.js",
    "2025": "rm -rf ./Results.txt && node src/AoC25/AoC2025solve.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:2021": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=src/AoC21",
    "test:2022": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=src/AoC22",
    "test:2023": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=src/AoC23",
    "test:2024": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=src/AoC24",
    "test:2025": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=src/AoC25",
    "test:utils": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=src/Utils"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmorsebrown/adventOfCode.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kmorsebrown/adventOfCode/issues"
  },
  "homepage": "https://github.com/kmorsebrown/adventOfCode#readme",
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "eslint": "^8.26.0",
    "jest": "^29.3.1",
    "prettier": "2.8.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "ts-jest": "^29.1.1"
  }
}
