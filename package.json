{
  "name": "adventofcode",
  "version": "1.0.0",
  "description": "Advent of Code",
  "main": "index.js",
  "scripts": {
    "2021": "rm -rf ./Results.txt && node src/AoC21/AoC2021solve.js",
    "2022": "rm -rf ./Results.txt && node src/AoC22/AoC2022solve.js",
    "2023": "rm -rf ./dist && mkdir ./dist && tsc && node dist/AoC23/AoC2023solve.js",
    "test": "jest",
    "test:2021": "jest --testPathPattern=src/AoC21 --collectCoverage=true --collectCoverageFrom=src/AoC21/**/*.{ts,js}",
    "test:2022": "jest --testPathPattern=src/AoC22 --collectCoverage=true --collectCoverageFrom=src/AoC22/**/*.{ts,js}",
    "test:2023": "jest --testPathPattern=src/AoC23 --collectCoverage=true --collectCoverageFrom=src/AoC23/**/*.{ts,js}",
    "test:utils": "jest --testPathPattern=src/Utils --collectCoverage=true --collectCoverageFrom=src/Utils/**/*.{ts,js}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmorsebrown/adventOfCode.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kmorsebrown/adventOfCode/issues"
  },
  "homepage": "https://github.com/kmorsebrown/adventOfCode#readme",
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "eslint": "^8.26.0",
    "jest": "^29.3.1",
    "prettier": "2.8.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "ts-jest": "^29.1.1"
  }
}
